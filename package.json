{
  "name": "mspretro-client",
  "version": "5.0.0",
  "description": "The client for MSPRetro",
  "main": "main.js",
  "scripts": {
    "start": "electron . --no-warnings",
    "build-windows": "electron-packager . MSPRetro --platform=win32 --arch=x64",
    "build-darwin": "electron-packager . MSPRetro --arch=x64 --asar-unpack='plugins/PepperFlashPlayer.plugin' --platform=darwin"
  },
  "author": "Polo",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "electron-packager": "^15.2.0",
    "node-fetch": "^2.6.5",
    "xml2js": "^0.4.23"
  }
}
